{
  "version": 2,
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "api/index.js"
    }
  ],
  "crons": [
    {
      "path": "/api",
      "schedule": "* * * * *"
    }
  ]
}
